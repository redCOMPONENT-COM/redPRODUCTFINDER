<?xml version="1.0" encoding="utf-8"?>
<form>
    <fieldset addfieldpath="/administrator/components/com_categories/models/fields">
		<field name="id"
			type="text"
			label="JGLOBAL_FIELD_ID_LABEL"
			description="JGLOBAL_FIELD_ID_DESC"
			size="10"
			default="0"
			readonly="true"
			class="readonly"
		/>

		<field name="formname"
			type="text"
			label="COM_REDPRODUCTFINDER_FORM_NAME"
		 />

		<field name="classname"
			type="text"
			label="CSS Classname"
		/>

		<field name="dependency"
			type="list"
			label="Dependancies"
			description="COM_CONTACT_FIELD_LINKED_USER_DESC">
			<option value="1">
				JYES</option>
			<option value="0">
				JNO</option>
		</field>

		<field id="published"
			name="published"
			type="list"
			label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC"
			class="chzn-color-state"
			size="1"
			default="1">
			<option value="1">
				JYES</option>
			<option value="0">
				JNO</option>
		</field>
		
		<field id="showname"
			name="showname"
			type="list"
			label="Show form name"
			class="chzn-color-state"
			size="1"
			default="0">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
		
		<field name="checked_out"
			type="text"
			label="COM_CONTACT_FIELD_LINKED_USER_LABEL"
			description="COM_CONTACT_FIELD_LINKED_USER_DESC"
		/>
		<field name="checked_out_time"
			type="text"
			label="COM_CONTACT_FIELD_LINKED_USER_LABEL"
			description="COM_CONTACT_FIELD_LINKED_USER_DESC"
		/>
	</fieldset>
</form>
